{
  "buildCommand": "cd matrixjupiter && npm run build",
  "outputDirectory": "matrixjupiter/.next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/styles.css",
      "destination": "/styles.css"
    },
    {
      "source": "/script.js",
      "destination": "/script.js"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
